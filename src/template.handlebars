<?xml version="1.0" encoding="UTF-8" ?>
<test-run id="2" name="TestCafe Tests" start-time="{{startTime}}" end-time="{{endTime}}">
    {{#fixtures}}
    <test-suite type="Assembly" name="{{name}}" start-time="{{startTime}}" end-time="{{endTime}}">
        {{#testCases}}
        <test-case name="{{name}}" fullname="{{../name}} - {{name}}" result="{{result}}" duration="{{duration}}">
            {{#if hasFailureData}}
            <failure>
                <message>
                    <![CDATA[{{{errorMessage}}}]]>
                </message>
                <stack-trace>
                    <![CDATA[{{{formattedErrorMessage}}}]]>
                </stack-trace>
            </failure>
            {{/if}}
            {{#if testRunInfo.screenshots}}
            <attachments>
                {{#testRunInfo.screenshots}}
                <attachment>
                    <filePath>{{screenshotPath}}</filePath>
                </attachment>
                {{/testRunInfo.screenshots}}
            </attachments>
            {{/if}}
        </test-case>
        {{/testCases}}
    </test-suite>
    {{/fixtures}}
</test-run>